{
  "name": "formeleven",
  "version": "0.0.1",
  "description": "",
  "keywords": "",
  "license": "",
  "author": "",
  "scripts": {
    "build": "npm run init && npm run build-elm && npm run build-html && npm run build-frontend",
    "build-elm": "elm make ./src/Build.elm --output /var/tmp/elm.js && sed \"s/'REPLACE_ME_WITH_JSON_STRINGIFY'/JSON.stringify(x)/g\" /var/tmp/elm.js > ./build/elm.js",
    "build-frontend": "parcel build 'frontend/*.html'",
    "build-html": "node build/index.js",
    "clean-dist": "rm -rf dist/*",
    "copy-files": "cp static/* dist",
    "dev": "parcel 'frontend/*.html' 'frontend/**/*.html'",
    "init": "npm run clean-dist && npm run copy-files",
    "start": "npm run init && npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.3.2",
    "@parcel/transformer-elm": "^2.3.2",
    "@parcel/transformer-webmanifest": "^2.3.2",
    "parcel": "^2.0.1"
  },
  "dependencies": {
    "elm": "^0.19.1-5",
    "filendir": "^2.0.1"
  }
}
